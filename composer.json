{
    "name": "ijpatricio/stackable-editor",
    "description": "A content editor built as a stack of blocks, in Livewire and Filament components.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ijpatricio\\StackableEditor\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Joao Patricio",
            "email": "ijpatricio@gmail.com"
        }
    ],
    "require": {},
    "extra": {
        "laravel": {
            "providers": [
                "Ijpatricio\\StackableEditor\\StackableEditorServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
